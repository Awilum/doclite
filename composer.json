{
    "name": "dwgebler/doclite",
    "description": "A powerful NoSQL data store built on top of SQLite",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "swaggest/json-schema": "^0.12.31",
        "symfony/property-access": "^5.2",
        "symfony/property-info": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/uid": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.6",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {"Gebler\\Doclite\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Gebler\\Doclite\\Tests\\": "tests/" }
    },
    "license": "GPL-3.0-or-commercial",
    "authors": [
        {
            "name": "David Gebler"
        }
    ]
}
